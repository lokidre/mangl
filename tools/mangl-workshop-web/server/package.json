  "private": true,
  "comment:type": "module",
  "main": "src/index.ts",
  "scripts": {
    "check-types": "tsc",
    "transpile": "babel ./src --out-dir ./build --extensions .ts",
    "start-dist": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/index.js",
    "build": "npm-run-all transpile",
    "server": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "rollup -c --environment BUILD:dev",
    "watch-dev-rollup": "rollup -c --environment BUILD:dev --watch",
    "start-debug": "run-p watch-dev start-server",
    "start-server": "st --no-cache -H 0.0.0.0 --port 9966 --index index.html .",
    "start-bak": "run-p start-server",
    "watch-dev": "nodemon --exec ts-node ./src/index.ts",
    "start": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "serve": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "help": "echo some help"
  },
